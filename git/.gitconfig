[core]
	pager = diff-so-fancy | less --tabs=4 -RF
	editor = nvim
[interactive]
	diffFilter = diff-so-fancy --patch
[merge]
	conflictStyle = diff3
[user]
	email = zeltronsrikar@gmail.com
	name = 231tr0n
[init]
	defaultBranch = main
[pull]
	rebase = false
[color]
	ui = true
[color "diff-highlight"]
	oldNormal = red bold
	oldHighlight = red bold 52
	newNormal = green bold
	newHighlight = green bold 22
[color "diff"]
	meta = 11
	frag = magenta bold
	func = 146 bold
	commit = yellow bold
	old = red bold
	new = green bold
	whitespace = red reverse
[push]
	autoSetupRemote = true
